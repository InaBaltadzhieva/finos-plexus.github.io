{
  "extends": "airbnb-base",
  "root": true,
  "env":
  {
    "node": true,
    "browser": true
  },
  "rules":
  {
    "no-plusplus": 0,
    "global-require": 0,
    "no-console": 0,
    "func-names": 0,
    "space-before-function-paren": 0,
    "max-len": 0,
    "no-restricted-syntax": 0,
    "object-curly-newline": 0,
    "no-alert": 0,
    "no-underscore-dangle": 0
  },
  "globals":
  {
    "List": true,
    "CanvasJS": true,
    "InteropPlatform": true,
    "platformFactory": true,
    "$": true,
    "appLogic": true
  }
}
